{
  "compilerOptions": {
    "target": "es6",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "baseUrl": "../",
    "paths": {
      "@virtonetwork/sdk/descriptors": ["../.papi/descriptors/dist/index.d.ts"]
    },
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
